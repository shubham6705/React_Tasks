       //Question 3
       const conseArr=[1,2,3,4,5,5,6];
       var count=0;
       for(let i=0;i<conseArr.length;i++)
       {
           for(let j=i+1;j<conseArr.length;j++)
           {
               if(conseArr[i]==conseArr[j])
               {
                    console.log(conseArr[i]);  
               }
           }
       }
